@model IEnumerable<MVC_01.Models.Customer>

@{ ViewBag.Title = "Germany Customers"; }

<h2 style="color:red">Germany Customers</h2>

<table class="table">
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>Contact Name</th>
        
    </tr>
    @foreach (var customer in Model)
    {
<tr>
    <td>@customer.CustomerID</td>
    <td>@customer.CompanyName</td>
    <td>@customer.ContactName</td>
    
</tr>
}
</table>